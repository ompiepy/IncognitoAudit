# Midnight Protocol Configuration
# Copy this file to .env and fill in your actual values

# Midnight Network Configuration
MIDNIGHT_RPC_URL="https://testnet.midnight.network"
MIDNIGHT_NETWORK_ID="testnet"
MIDNIGHT_API_KEY="your_midnight_api_key_here"

# Midnight Proof Server (for ZK proof generation)
MIDNIGHT_PROOF_SERVER_URL="https://proof-server-testnet.midnight.network"

# Wallet Configuration
# Your Midnight wallet private key (keep this secure!)
MIDNIGHT_PRIVATE_KEY="your_wallet_private_key_here"
MIDNIGHT_WALLET_ADDRESS="your_wallet_address_here"
MIDNIGHT_MNEMONIC="your_wallet_mnemonic_phrase_here"

# Contract Addresses (after deployment)
ZK_COMPLIANCE_CONTRACT_ADDRESS="0x..."

# Encryption Key for Private State Manager
ENCRYPTION_KEY="your_32_byte_encryption_key_here"

# Environment
NODE_ENV="development"
MIDNIGHT_USE_OFFICIAL_SDK="true"
